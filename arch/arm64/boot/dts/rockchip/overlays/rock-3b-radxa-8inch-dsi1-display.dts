/dts-v1/;
/plugin/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/display/drm_mipi_dsi.h>
#include "dt-bindings/display/drm_mipi_dsi_overlay.h"

/ {
	metadata {
		title ="Enable Radxa Display 8HD on DSI1";
		compatible = "radxa,rock-3b";
		category = "display";
		exclusive = "dsi1";
		description = "Enable Radxa Display 8HD on DSI1.
pin02 for panel Vcc-5v.
pin21 for panel backlight Pwm.	
pin25 for panel Gnd.
pin29 for tp Int.
pin31 for tp Rst.";
	};
};

&{/} {
	vcc_lcd_dsi1: vcc-lcd-dsi1 {
		status = "okay";
		compatible = "regulator-fixed";
		regulator-name = "vcc_lcd_mipi_dsi1";
		gpio = <&gpio0 RK_PC7 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-always-on;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	backlight_dsi1: backlight-dsi1 {
		status = "okay";
		compatible = "pwm-backlight";
		pwms = <&pwm12 0 25000 0>;
		brightness-levels = <0 255>;
		num-interpolated-steps = <255>;  
		default-brightness-level = <250>;
	};
};

&pwm12 {
	status = "okay";
	pinctrl-names = "active";
	pinctrl-0 = <&pwm12m1_pins>;
};

