&video_phy1 {
	status = "okay";
};

&dsi1_in_vp0 {
	status = "disabled";
};

&dsi1_in_vp1 {
	status = "okay";
};

&vp0_out_dsi1 {
	status = "disabled";
};

&vp1_out_dsi1 {
	status = "okay";
};

&route_dsi1 {
	status = "okay";
	connect = <&vp1_out_dsi1>;
};

&i2c3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c3m1_xfer>;
	clock-frequency = <400000>;
	#address-cells = <1>;
	#size-cells = <0>;

	gt9xx: gt9xx@14 {
		status = "okay";
		compatible = "goodix,gt9xx";
		reg = <0x14>;
		pinctrl-names = "default";
		pinctrl-0 = <&gt9xx_gpio>;
		touch-gpio = <&gpio2 RK_PD7 IRQ_TYPE_LEVEL_HIGH>;
		reset-gpio = <&gpio3 RK_PA0 GPIO_ACTIVE_HIGH>;
		max-x = <800>;
		max-y = <1280>;
		tp-size = <9112>;
	};
};

&pinctrl {
	gt9xx {
		gt9xx_gpio: gt9xx-gpio {
			rockchip,pins =
				<3 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 RK_PD7 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};
