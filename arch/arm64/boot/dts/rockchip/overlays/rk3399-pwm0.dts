/dts-v1/;
/plugin/;

/ {
        compatible = "rockchip,rk3399-pwm", "rockchip,rk3288-pwm";

        metadata {
                title = "Enable pwm0";
                compatible = "rockchip,rk3399";
                category = "misc";
                description = "Enable pwm0";
        };

        fragment@0 {
                target = <&pwm0>;

                __overlay__ {
                        status = "okay";
                };
        };
};
