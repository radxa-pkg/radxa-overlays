/dts-v1/;
/plugin/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pinctrl/rockchip.h>

/ {
        metadata {
                title = "Enable PWM0";
                compatible = "rockchip,rk3399";
                category = "misc";
                description = "Enable PWM0";
        };

        fragment@0 {
                target = <&pwm0>;

                __overlay__ {
                        status = "okay";
                };
        };
};
