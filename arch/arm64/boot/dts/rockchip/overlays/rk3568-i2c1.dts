/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable I2C1";
		compatible = "unknown";
		category = "misc";
		description = "Enable I2C4-m0 on pin 36 pin 40 of radxa e23";
	};

	fragment@0 {
		target = <&i2c1>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c1_xfer>;
		};
	};
};
