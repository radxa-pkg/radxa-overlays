/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable I2C4-M0";
		compatible = "radxa,rock-3c", "radxa,e23";
		category = "misc";
		description = "Enable I2C4-M0.\nOn ROCK 3C this is SDA pin 27 and SCL pin 28.\nOn RADXA E23 and RADXA CM3 IO BOARD this is SDA pin 19 and SCL pin 23.";
	};

	fragment@0 {
		target = <&i2c4>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c4m0_xfer>;
		};
	};
};
