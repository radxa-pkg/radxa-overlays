/dts-v1/;
/plugin/;

/ {
	metadata {
		title ="Set OTG port to Host mode";
		compatible = "rockchip,rk3399";
		category = "misc";
		description = "Set OTG port to Host mode.\nUse this when you want to connect USB devices.";
	};

	fragment@0 {
		target = <&usbdrd_dwc3_0>;

		__overlay__ {
			status = "okay";
			dr_mode = "host";
			extcon = <&u2phy0>;
		};
	};
};
