/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART6-M1";
		compatible = "radxa,rock-5a";
		category = "misc";
		exclusive= "GPIO1_A1", "GPIO1_A0";
		description = "Enable UART6-M1.";
	};

	fragment@0 {
		target = <&uart6>;

		__overlay__ {
			status = "okay";
			pinctrl-0 = <&uart6m1_xfer>;
		};
	};
};
