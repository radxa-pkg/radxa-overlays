/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable FIQ Debugger on UART8-M1";
		compatible = "radxa,rock-3b";
		category = "misc";
		description = "Enable FIQ Debugger on UART8-M1.\nOn Radxa ROCK 3B this is TX pin 29 and this is RX pin 31.";
	};

	fragment@0 {
		target = <&fiq_debugger>;

		__overlay__ {
			status = "okay";
			rockchip,serial-id = <8>;
			pinctrl-names = "default";
			pinctrl-0 = <&uart8m1_xfer>;
		};
	};

	fragment@1 {
		target = <&uart8>;

		__overlay__ {
			status = "disabled";
		};
	};
};
