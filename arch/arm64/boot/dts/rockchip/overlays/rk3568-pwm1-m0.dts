/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM1-M0";
		compatible = "radxa,rock-3b";
		category = "misc";
		exclusive = "GPIO0_C0";
		description = "Enable PWM1-M0.\nOn Radxa ROCK 3B this is pin 15.";
	};

	fragment@0 {
		target = <&pwm1>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "active";
			pinctrl-0 = <&pwm1m0_pins>;
		};
	};
};
