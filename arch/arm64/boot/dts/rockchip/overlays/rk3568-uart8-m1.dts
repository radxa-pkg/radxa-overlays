/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART8-M1";
		compatible = "radxa,rock-3b";
		category = "misc";
		description = "Enable UART8-M1.\nOn Radxa ROCK 3B this is TX pin 29 and RX pin 31.";
	};

	fragment@0 {
		target = <&uart8>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&uart8m1_xfer>;
		};
	};
};
