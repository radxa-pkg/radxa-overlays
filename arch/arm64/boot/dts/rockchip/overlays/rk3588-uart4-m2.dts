/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART4-M2";
		compatible = "radxa,rock-5a";
		category = "misc";
		exclusive= "GPIO1_B3", "GPIO1_B2";
		description = "Enable UART4-M2.\nOn Radxa ROCK5A Hardware X1.2 this is TX pin 7 and RX pin 29.";
	};

	fragment@0 {
		target = <&uart4>;

		__overlay__ {
			status = "okay";
			pinctrl-0 = <&uart4m2_xfer>;
		};
	};
};
